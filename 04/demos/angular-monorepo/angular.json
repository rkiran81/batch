{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"cli": {
		"analytics": false
	},
	"version": 1,
	"newProjectRoot": "apps",
	"projects": {
		"shell": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"inlineStyle": true,
					"skipTests": true
				},
				"@schematics/angular:class": {
					"skipTests": true
				},
				"@schematics/angular:directive": {
					"skipTests": true
				},
				"@schematics/angular:guard": {
					"skipTests": true
				},
				"@schematics/angular:interceptor": {
					"skipTests": true
				},
				"@schematics/angular:pipe": {
					"skipTests": true
				},
				"@schematics/angular:resolver": {
					"skipTests": true
				},
				"@schematics/angular:service": {
					"skipTests": true
				},
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "apps/shell",
			"sourceRoot": "apps/shell/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/shell",
						"index": "apps/shell/src/index.html",
						"main": "apps/shell/src/main.ts",
						"polyfills": "apps/shell/src/polyfills.ts",
						"tsConfig": "apps/shell/tsconfig.app.json",
						"assets": [
							"apps/shell/src/favicon.ico",
							"apps/shell/src/assets"
						],
						"styles": [
							"apps/shell/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "apps/shell/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/shell/src/environments/environment.ts",
									"with": "apps/shell/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "apps/shell/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "shell:build:production",
							"extraWebpackConfig": "apps/shell/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "shell:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "apps/shell/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "shell:build",
						"extraWebpackConfig": "apps/shell/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "apps/shell/src/test.ts",
						"polyfills": "apps/shell/src/polyfills.ts",
						"tsConfig": "apps/shell/tsconfig.spec.json",
						"karmaConfig": "apps/shell/karma.conf.js",
						"assets": [
							"apps/shell/src/favicon.ico",
							"apps/shell/src/assets"
						],
						"styles": [
							"apps/shell/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		},
		"shared": {
			"projectType": "library",
			"root": "libs/shared",
			"sourceRoot": "libs/shared/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "libs/shared/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "libs/shared/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "libs/shared/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "libs/shared/src/test.ts",
						"tsConfig": "libs/shared/tsconfig.spec.json",
						"karmaConfig": "libs/shared/karma.conf.js"
					}
				}
			}
		},
		"mfe-basket": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"inlineStyle": true,
					"skipTests": true
				},
				"@schematics/angular:class": {
					"skipTests": true
				},
				"@schematics/angular:directive": {
					"skipTests": true
				},
				"@schematics/angular:guard": {
					"skipTests": true
				},
				"@schematics/angular:interceptor": {
					"skipTests": true
				},
				"@schematics/angular:pipe": {
					"skipTests": true
				},
				"@schematics/angular:resolver": {
					"skipTests": true
				},
				"@schematics/angular:service": {
					"skipTests": true
				},
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "apps/mfe-basket",
			"sourceRoot": "apps/mfe-basket/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/mfe-basket",
						"index": "apps/mfe-basket/src/index.html",
						"main": "apps/mfe-basket/src/main.ts",
						"polyfills": "apps/mfe-basket/src/polyfills.ts",
						"tsConfig": "apps/mfe-basket/tsconfig.app.json",
						"assets": [
							"apps/mfe-basket/src/favicon.ico",
							"apps/mfe-basket/src/assets"
						],
						"styles": [
							"apps/mfe-basket/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "apps/mfe-basket/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/mfe-basket/src/environments/environment.ts",
									"with": "apps/mfe-basket/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "apps/mfe-basket/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "mfe-basket:build:production",
							"extraWebpackConfig": "apps/mfe-basket/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "mfe-basket:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"publicHost": "http://localhost:4201",
						"extraWebpackConfig": "apps/mfe-basket/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "mfe-basket:build",
						"extraWebpackConfig": "apps/mfe-basket/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "apps/mfe-basket/src/test.ts",
						"polyfills": "apps/mfe-basket/src/polyfills.ts",
						"tsConfig": "apps/mfe-basket/tsconfig.spec.json",
						"karmaConfig": "apps/mfe-basket/karma.conf.js",
						"assets": [
							"apps/mfe-basket/src/favicon.ico",
							"apps/mfe-basket/src/assets"
						],
						"styles": [
							"apps/mfe-basket/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		}
  }
}